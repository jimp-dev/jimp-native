{
  "name": "@jimp-native/plugins-napi",
  "version": "0.1.0-alpha.7",
  "description": "Default jimp plugins with all available C++ optimisations enabled using the Node Addon API",
  "main": "dist/index.js",
  "module": "es/index.js",
  "types": "src/index.ts",
  "scripts": {
    "clean": "rimraf ./dist ./es ./build",
    "build": "npm run clean && npm run build:node && npm run build:module",
    "build:watch": "npm run build:node:debug -- -- --watch --verbose",
    "build:debug": "npm run build:node:debug",
    "build:module": "cross-env BABEL_ENV=module babel src -d es --extensions \".ts\" --source-maps --config-file ../../babel.config.js",
    "build:node": "babel src -d dist --source-maps --extensions \".ts\" --config-file ../../babel.config.js"
  },
  "author": "Sjoerd Dal",
  "license": "MIT",
  "dependencies": {
    "@jimp-native/plugin-blit-napi": "^0.1.0-alpha.7",
    "@jimp-native/plugin-blur-napi": "^0.1.0-alpha.7",
    "@jimp-native/plugin-circle-napi": "^0.1.0-alpha.7",
    "@jimp-native/plugin-color-napi": "^0.1.0-alpha.7",
    "@jimp-native/plugin-composite-napi": "^0.1.0-alpha.7",
    "@jimp-native/plugin-crop-napi": "^0.1.0-alpha.7",
    "@jimp-native/plugin-dither-napi": "^0.1.0-alpha.7",
    "@jimp-native/plugin-flip-napi": "^0.1.0-alpha.7",
    "@jimp-native/plugin-invert-napi": "^0.1.0-alpha.7",
    "@jimp-native/plugin-mask-napi": "^0.1.0-alpha.7",
    "@jimp-native/plugin-resize-napi": "^0.1.0-alpha.7",
    "@jimp-native/plugin-rotate-napi": "^0.1.0-alpha.7",
    "@jimp/core": "^0.22.7",
    "@jimp/plugin-contain": "^0.22.7",
    "@jimp/plugin-cover": "^0.22.7",
    "@jimp/plugin-displace": "^0.22.7",
    "@jimp/plugin-fisheye": "^0.22.7",
    "@jimp/plugin-gaussian": "^0.22.7",
    "@jimp/plugin-normalize": "^0.22.7",
    "@jimp/plugin-print": "^0.22.7",
    "@jimp/plugin-scale": "^0.22.7",
    "@jimp/plugin-shadow": "^0.22.7",
    "@jimp/plugin-threshold": "^0.22.7",
    "timm": "^1.7.1"
  },
  "devDependencies": {
    "@jimp/core": "^0.22.7"
  },
  "publishConfig": {
    "access": "public"
  }
}
